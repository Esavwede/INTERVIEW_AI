    "connect-redis": "^7.1.1",
    "cookie-parser": "^1.4.6",
    "cookie-session": "^2.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "express-async-errors": "^3.1.1",
    "express-handlebars": "^8.0.1",
    "express-session": "^1.18.0",
    "form-data": "^4.0.0",
    "handlebars": "^4.7.8",
    "helmet": "^7.1.0",
    "inserturlparams": "^2.0.4",
    "jet-logger": "^1.3.1",
    "jet-paths": "^1.0.9",
    "js-yaml": "^4.1.0",
    "jsonfile": "^6.1.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mailersend": "^2.3.0",
    "mailgun.js": "^10.2.3",
