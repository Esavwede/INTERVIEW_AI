{
  "name": "INTERVIEW_AI",
  "version": "0.0.0",
  "scripts": {
    "build": "npx ts-node build.ts && npm run sentry:sourcemaps",
    "lint": "npx eslint --ext .ts src/",
    "lint:tests": "npx eslint --ext .ts spec/",
    "start": "node -r module-alias/register ./dist --env=production",
    "dev": "nodemon | pino-pretty",
    "test": "jest",
    "test:no-reloading": "npx ts-node --files -r tsconfig-paths/register ./spec",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org nodejs-nf --project interview-ai dist && sentry-cli sourcemaps upload --org nodejs-nf --project interview-ai dist"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts, html",
    "ignore": [
      "src/public"
    ],
    "exec": "./node_modules/.bin/ts-node --files -r tsconfig-paths/register ./src"
  },
  "_moduleAliases": {
    "@src": "dist"
  },
  "engines": {
    "node": ">=8.10.0"
  },
  "dependencies": {
    "@getbrevo/brevo": "^2.2.0",
    "@sendinblue/client": "^3.3.1",
